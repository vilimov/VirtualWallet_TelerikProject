<div class="w3-row-padding">
	<div class="w3-col m12">
		<div class="w3-card w3-round w3-white w3-center">
			<div class="w3-container">
				<h3 class="h4shadowRed"> Something went wrong! </h3>
				<h4 class="h4shadowRed" style="color:red"> @ViewData["ErrorMessage"]</h4>
				<p class="h4shadowRed"> Status code: @this.Context.Response.StatusCode </p>
				<button class="w3-button w3-blue" onclick="goBack()">Go Back</button>
			</div>
		</div>
	</div>
</div>

<script>
	function goBack() {
		window.history.back();
	}
</script>